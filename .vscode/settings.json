{
    "files.exclude": {
        "**/.npmrc": true,
        "**/*.tgz": true,
        "**/block-pack": true,
        "**/dist": true,
        "**/index.d.ts": true,
        "**/index.js": true,
        "**/node_modules": true,
        "**/pnpm-lock.yaml": true,
        "**/tsconfig.app.json": true,
        "**/tsconfig.json": true,
        "**/tsconfig.node.json": true,
        "**/vite.config.mts": true,
        "**/vite.config.ts": true,
        "**/workflow/dev": true
    },
    "vitest.disableWorkspaceWarning": true,
    "prettier.singleAttributePerLine": true,
    "vue.format.wrapAttributes": "auto",
    "editor.defaultFormatter": "dbaeumer.vscode-eslint",
    "[typescript]": {
        "editor.defaultFormatter": "dbaeumer.vscode-eslint"
    },
    "[vue]": {
        "editor.defaultFormatter": "dbaeumer.vscode-eslint"
    },
    "cSpell.words": [
        "amplicon",
        "blastn",
        "blastp",
        "clns",
        "Clonotype",
        "clonotyping",
        "Conv",
        "covariate",
        "datasource",
        "dendro",
        "Dendrogram",
        "dendrograms",
        "dendros",
        "Downsampling",
        "Fastq",
        "isotype",
        "pframe",
        "postanalysis",
        "prerun",
        "ptable",
        "spectratype",
        "strandness",
        "subread",
        "umap",
        "mmseqs"
    ],
    "prettier.requireConfig": false,
    "[python]": {
        "editor.defaultFormatter": "ms-python.autopep8"
    }
}